{
  "manifest_version": 3,
  "name": "Binance Token Extractor",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "storage",
    "activeTab",
    "cookies"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://www.binance.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}